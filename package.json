  },
  "homepage": "https://github.com/FavioSauto/trivia-search-api#readme",
  "devDependencies": {
    "babel-jest": "^26.3.0",
    "babel-loader": "8.1.0",
    "css-loader": "4.3.0",
    "enzyme": "^3.11.0",
